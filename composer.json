{
    "name": "atasciuc/zend-expressive-validation",
    "type": "library",
    "description": "PSR-7 Validation middleware for zend-expressive",
    "authors": [
        {
            "name": "Alexandr Tasciuc"
        }
    ],
    "license": "BSD-3-Clause",
    "repositories": [
    ],
    "keywords": [
        "http",
        "zend",
        "expressive",
        "validation",
        "psr",
        "psr-7"
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "1.0",
        "zendframework/zend-stdlib": "^3.0",
        "doctrine/orm": "^2.5",
        "zendframework/zend-validator": "^2.5",
        "zendframework/zend-http": "^2.5",
        "zendframework/zend-i18n": "^2.5",
        "zendframework/zend-filter": "^2.5",
        "symfony/serializer": "^2.7",
        "doctrine/doctrine-module": "^2.*",
        "mtdowling/cron-expression": "^1.0",
        "nesbot/carbon": "^1.21",
        "fzaninotto/faker": "~1.5.0",
        "los/api-problem": "^0.9.0"
    },
    "require-dev": {
        "composer/composer": ">=1.0.0-alpha10",
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "filp/whoops": "^1.1",
        "robmorgan/phinx": "^0.4.6",
        "fzaninotto/faker": "^1.5",
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-4": {
            "ExpressiveValidator\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ExpressiveValidator\\Test\\": "tests/unit/ExpressiveValidator/src/Test",
            "ExpressiveValidatorFixture\\Test\\": "tests/unit/fixtures"
        }
    },
    "scripts": {
        "test": [
            "php vendor/squizlabs/php_codesniffer/scripts/phpcs"
        ]
    }
}
